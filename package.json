{
  "name": "vite-template-react",
  "version": "1.2.0",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "cypress open --e2e",
    "cy:open": "cypress open --e2e",
    "em:start": "firebase emulators:start --import=./saved-data --export-on-exit",
    "em:exec": "env VITE_EMULATE=true firebase emulators:exec --import=./saved-data 'npm start'",
    "em:cy:open": "start-server-and-test em:exec http://127.0.0.1:3000 cy:open"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.8.0",
    "firebase": "^9.12.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/react": "^13.3.0",
    "@vitejs/plugin-react": "^2.0.0",
    "cypress": "^11.0.0",
    "jsdom": "^20.0.0",
    "start-server-and-test": "^1.14.0",
    "vite": "^3.0.3",
    "vitest": "^0.19.1"
  }
}
